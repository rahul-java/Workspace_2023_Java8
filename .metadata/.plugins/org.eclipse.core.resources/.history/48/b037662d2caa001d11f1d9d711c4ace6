package com.app.lambda_expression2;

public class Test3 {

	public void testMethod()
	{
		InterfaceA x=(int age)->
		                {
			               System.out.println(" Hello This is my first Lambda expression...");
		                 };
		x.checkAge(17);
	}
	public static void main(String[] args) {
		new Test1().testMethod();
	}

}
