package com.app.optional_class;

import java.util.Optional;

public class ClassA {

	void meth1()
	{
		String arr[]=new String[5];
		arr[1]="Rahul";
		arr[1]="Java";
		
		Optional<String> op = Optional.ofNullable(arr[2]);
		System.out.println(op);
	}
	public static void main(String[] args) {
		
		new ClassA().meth1();

	}

}
